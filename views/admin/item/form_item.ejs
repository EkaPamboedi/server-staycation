<div class="tab-pane fade" id="add-item" role="tabpanel" aria-labelledby="profile-tab">
          <h5 class="card-title">Add Bank</h5>
    <form action="/admin/bank" method="POST" enctype="multipart/form-data">
                <div class="form-group">
                  <label for="bankName">Bank Name</label>
                  <input type="text" class="form-control" id="" name="bankName" placeholder="Enter Bank Name" required>
                  
                  <label for="bankNumber">Bank Number</label>
                  <input type="text" class="form-control" id="" name="bankNumber" placeholder="Enter Bank Number" required>
                  
                  <label for="name">Name</label>
                  <input type="text" class="form-control" id="" name="name" placeholder="Enter Name" required>

                  <label for="imageUrl">Bank Image </label>
                  <input type="file" class="form-control" id="" name="image" required>
                </div>
                
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary">Submit</button>
              </form>
            </div>
</div>